{
    "type": "module",
    "dependencies": {
        "@neondatabase/serverless": "^1.0.1",
        "@prisma/client": "^6.15.0",
        "bcrypt": "^6.0.0",
        "body-parser": "^2.2.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "pg": "^8.16.3"
    },
    "scripts": {
        "dev": "nodemon --watch src --exec \"node --loader ts-node/esm src/index.ts\"",
        "build": "tsc",
        "start": "node dist/server.js",
        "migrate:make": "knex migrate:make --knexfile knexfile.ts --esm",
        "migrate:latest": "knex migrate:latest --knexfile knexfile.ts --esm",
        "seed:run": "knex seed:run --knexfile knexfile.ts --esm",
        "migrate": "ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js --knexfile src/db/knexfile.ts"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/knex": "^0.15.2",
        "@types/node": "^24.3.0",
        "nodemon": "^3.1.10",
        "prisma": "^6.15.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.2"
    }
}
